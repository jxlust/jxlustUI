{
  "name": "jxlustui",
  "version": "1.0.0",
  "description": "jxlust Vue3 UIåº“",
  "main": "lib/bundle.esm.js",
  "jsnext:main": "lib/bundle.esm.js",
  "module": "lib/bundle.esm.js",
  "browser": "dist/index.js",
  "engines": {
    "node": "^12 || >=14"
  },
  "scripts": {
    "bootstrap": "pnpm install",
    "serve:demo": "cross-env --max_old_space_size=4096 vite",
    "dev": "npx rollup -w -m -c rollup.config.dev.js",
    "build": "rollup -c rollup.config.js && npm run copy",
    "publish": "node scripts/publish.js",
    "copy": "cp package.json README.md lib",
    "test": "ts-node test/test.ts",
    "lint": "eslint 'src/**/*.{js,ts}'",
    "lint:eslint": "eslint --cache --max-warnings 0  \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --cache --fix --allow-empty-input \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && npm run bootstrap",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:jxlust/jxltools.git"
  },
  "keywords": [
    "rollup",
    "vue3",
    "UI",
    "typescript"
  ],
  "author": "jxlust",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.18.0",
    "@babel/preset-env": "^7.18.0",
    "@babel/preset-typescript": "^7.17.12",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-eslint": "^8.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "commander": "^9.2.0",
    "eslint": "^8.16.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "prettier": "^2.6.2",
    "rollup": "^2.74.1",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-scss": "^3.0.0",
    "rollup-plugin-serve": "^1.1.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "shelljs": "^0.8.5",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vue-template-compiler": "^2.6.14",
    "@commitlint/cli": "^17.0.1",
    "@commitlint/config-conventional": "^17.0.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^17.0.35",
    "@vitejs/plugin-legacy": "^1.8.2",
    "@vitejs/plugin-vue": "^2.3.3",
    "@vitejs/plugin-vue-jsx": "^1.3.10",
    "@vue/compiler-sfc": "^3.2.36",
    "autoprefixer": "^10.4.7",
    "commitizen": "^4.2.4",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.9",
    "eslint-config-prettier": "^8.5.0",
    "eslint-define-config": "^1.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^9.0.1",
    "filemanager-plugin": "^2.8.1",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.14",
    "postcss-html": "^1.4.1",
    "postcss-px-to-viewport-8-plugin": "^1.1.3",
    "sass": "^1.52.1",
    "stylelint": "^14.8.5",
    "stylelint-config-html": "^1.0.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended": "^7.0.0",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "unplugin-auto-import": "^0.8.5",
    "unplugin-vue-components": "^0.19.5",
    "unplugin-vue-define-options": "^0.6.1",
    "uuid": "^8.3.2",
    "vite": "^2.9.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-windicss": "^1.8.4",
    "vue-eslint-parser": "^9.0.2",
    "vue-tsc": "^0.34.7"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "core-js": "^3.22.5",
    "vue": "^3.2.35",
    "vue-loader": "^17.0.0",
    "vue-router": "^4.0.15",
    "@vueuse/core": "^8.5.0",
    "@vueuse/integrations": "^8.5.0",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.14"
  },
  "peerDependencies": {
    "postcss": "^8.4.14"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "{!(package)*.json,*.code-snippets,.!(browserslist)*rc}": [
      "prettier --write--parser json"
    ],
    "package.json": [
      "prettier --write"
    ],
    "*.vue": [
      "eslint --fix",
      "prettier --write",
      "stylelint --fix"
    ],
    "*.{scss,less,styl,html}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.md": [
      "prettier --write"
    ]
  }
}
